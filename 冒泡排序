#include<iostream>
using namespace std;
int main() {
	int arr[10] = { 5,6,2,3,0,1,4,8,7,9 };
	cout << "排序前：";
	for (int i = 0; i < 10; i++) {
		cout << arr[i];
	}
	cout << endl;
	for (int i = 0; i < 10 - 1; i++) {
		for (int j = 0; j < 10 - i - 1; j++) {
			if (arr[j] > arr[j + 1]) {
				int temp = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = temp;
			}
		}
	}
	cout << "排序后：";
	for (int i = 0; i < 10; i++) {
		cout << arr[i];
	}
	cout << endl;
}
